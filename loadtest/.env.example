# FormBridge k6 Load Test Configuration
# Copy this file to .env and fill in appropriate values

# Base URL for the API endpoint
# LOCAL: http://127.0.0.1:3000 (for local development)
# PROD: https://12mse3zde5.execute-api.ap-south-1.amazonaws.com/Prod
BASE_URL=http://127.0.0.1:3000

# API Key (if required by your endpoint)
API_KEY=

# Form ID for testing
FORM_ID=my-portfolio

# Enable HMAC signature headers (true/false)
HMAC_ENABLED=false

# HMAC secret key (only needed if HMAC_ENABLED=true)
HMAC_SECRET=
